{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "extends": ["airbnb"],
  "rules": {
    "max-len": ["error", {"code": 100}],
    "import/extensions": [
      "error",
      "always"
    ],
    "no-useless-escape": "off"
  },
  "env": {
    "browser": true,
    "jest": true,
    "es2021": true
  }
}